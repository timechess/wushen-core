{
  "capabilities": [
    {
      "identifier": "main-dialogs",
      "description": "Allow the main window to open/save files via the dialog plugin.",
      "remote": {
        "urls": ["http://localhost:3000/*", "http://127.0.0.1:3000/*"]
      },
      "windows": ["main"],
      "permissions": [
        "core:default",
        "dialog:allow-open",
        "dialog:allow-save",
        "app-commands"
      ]
    }
  ]
}
